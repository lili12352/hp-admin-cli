  "description": "一款能根据需求自动配置vue后台管理的脚手架",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "bd": "npm run build & npm run dev",
    "dev": "npm link & npm run clear:test & npm run cli",
    "build": " npm run clear:build  && tsc && node index.js",
